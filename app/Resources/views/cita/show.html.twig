{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cita</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ citum.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ citum.fecha }}</td>
            </tr>
            <tr>
                <th>Deposito</th>
                <td>{{ citum.deposito }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('citas_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('citas_edit', { 'id': citum.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
